# Recursive fibonacci number generator
module $fibonacci

func run($n) {
	var $prev = 1
	var $result = 1
	var $sum = 0
	
	for(
		var $i = 0, lt($i, $n), $i = add($i, 1),
		[
			$sum = add($result, $prev)
			$prev = $result
			$result = $sum
		]
	)
	
	$result
}

func run_recursive($n) {
	if(lteq($n,0),1,
		add(
			[
				run_recursive(sub($n,1))
			],
			[
				run_recursive(sub($n,2))
			]
		)
	)
}